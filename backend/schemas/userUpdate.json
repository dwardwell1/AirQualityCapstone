{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 2,
      "maxLength": 30
    },
    
    "email": {
      "type": "string",
      "minLength": 6,
      "maxLength": 60,
      "format": "email"
    },

    "password": {
      "type": "string",
      "minLength": 6,
      "maxLength": 60
    },
    "default_locale": {
      "type": "string",
      "minLength": 5,
      "maxLength": 5
    
    },
    "alerts": {
      "type": "string"
    },
    "isAdmin": {
      "type": "boolean"
    }


  },
  "additionalProperties": false,
  "required": []
}
