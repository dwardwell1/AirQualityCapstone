{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/jobNew.schema.json",
  "type": "object",
  "properties": {
    "zipcode": {
      "type": "integer"
    },
    "city": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "stateCode": {
      "type": "string",
      "minLength": 1,
      "maxLength": 5
    },
    "lat": {
      "type": "number",
      "minimum": -90,
      "maximum": 90
    },
    "lng": {
      "type": "number",
      "minimum": -180,
      "maximum": 180
    }
  },
  "additionalProperties": false,
  "required": [
    
  ]
}
